<html>

<head>

   <% include ../partials/head %>

</head>

<body>

    <div id="wrapper">

        <!-- Navigation -->
      
        <% include ../partials/navigation %>
        <div id="page-wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">RATTATA Sentiement analysis</h1>
                </div>
                <!-- /.col-lg-12 -->
            </div>
           
            <!-- /.row -->
            <div class="row">
                <div class="col-lg-8">
                    
                    <!-- /.panel -->
                    <div class="panel panel-default">
                        
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div class="row">
                                <div class="col-lg-4">
                                    <div class="table-responsive">
                                        <table class="table table-bordered table-hover table-striped">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>domaine</th>
                                                    <th>language</th>

                                                </tr>
                                            </thead>
                                            
                                            
                                            <tbody>
                                                 <% datas.forEach(function(data) { %>
                                                
                                                <tr>
                                                    <td> <a href ="edit_domaine?id=<%= data._id %>"> Details </a></td>
                                                    <td><%= data.domaine_name %></td>
                                                    <td><%= data.domaine_language %></td>

                                                </tr>  
                                                    
                                                 <% } )%>
                                                
                                                
                                               
                                            </tbody>
                                        </table>
                                    </div>
                                    <!-- /.table-responsive -->
                                </div>
                                <!-- /.col-lg-4 (nested) -->
                                <div class="col-lg-8">
                                    <div id="morris-bar-chart"></div>
                                </div>
                                <!-- /.col-lg-8 (nested) -->
                            </div>
                            <!-- /.row -->
                        </div>
                        <!-- /.panel-body -->
                    </div>
                    <!-- /.panel -->
                    
                    <!-- /.panel -->
                </div>
                              
                <!-- /.col-lg-4 -->
            </div>
            <!-- /.row -->
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <% include ../partials/footer %>
</body>

</html>
